{
    "version": "0.1.5",
    "name": "flowlog/flowlog-laravel",
    "description": "Laravel SDK for Flowlog - Async logging with automatic context extraction",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andreas G. Hjarn√∏",
            "email": "andreasgadej@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/queue": "^10.0|^11.0|^12.0",
        "monolog/monolog": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0|^11.0|^12.0",
        "pestphp/pest": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Flowlog\\FlowlogLaravel\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Flowlog\\FlowlogLaravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Flowlog\\FlowlogLaravel\\FlowlogServiceProvider"
            ],
            "aliases": {
                "Flowlog": "Flowlog\\FlowlogLaravel\\Facades\\Flowlog"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "keywords": [
        "laravel",
        "logging",
        "flowlog",
        "monolog",
        "async",
        "queue"
    ],
    "support": {
        "issues": "https://github.com/flowlog/flowlog-laravel/issues",
        "source": "https://github.com/flowlog/flowlog-laravel"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
